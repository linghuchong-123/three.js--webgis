{
  "presets": [
    ["@babel/preset-env", 
    { 
      "modules": false, 
      "targets": "> 1%", 
      "loose": true, 
      "bugfixes": true 
    }],
    ["@babel/preset-typescript"]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    ["@babel/plugin-proposal-private-methods",{"loose":false}],
    ["@babel/plugin-proposal-private-property-in-object",{"loose":false}]
  ]
}
